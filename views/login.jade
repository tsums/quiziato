extends layout

block content
    h1 Quiz App
    p Welcome to Quiz App, please Log In:
    div.loginbox
        form(name="login", action="/login", method="post")
            div
                label UCID:
                input(type="text", name="username")
            div
                label Password:
                input(type="password", name="password")
            div
                input(type="submit", value="login")
