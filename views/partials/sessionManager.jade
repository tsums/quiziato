h5 Review Previous Sessions
div.well
    form-group.form-group
        select.form-control(required='', ng-model='currentCourse', ng-options='course.title for course in API.courses')
            option(value='') Select Course

div.well
    form-group.form-group
        select.form-control(ng-model='currentSession', ng-options='session as getLabel(session) for session in sessions | orderBy: "-date"')
            option(value='') Choose Session

div(ng-repeat='record in sessionDetail.attendace')
    p {{record.student.name.full}}
